{
  "name": "country-index",
  "version": "1.0.0",
  "description": "Structured metadata for countries with ISO codes, dial codes, and regions.",
  "author": "Izzet Yildirim <izzetcyildirim@gmail.com>",
  "license": "MIT",
  "type": "commonjs",
  "homepage": "https://github.com/izzet/country-index#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/izzet/country-index.git"
  },
  "bugs": {
    "url": "https://github.com/izzet/country-index/issues"
  },
  "keywords": [
    "countries",
    "country",
    "iso2",
    "phone",
    "dial",
    "dial-code",
    "regions",
    "metadata",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "prepare": "husky install",
    "test": "jest",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{ts,js}": [
      "eslint --fix"
    ]
  }
}
